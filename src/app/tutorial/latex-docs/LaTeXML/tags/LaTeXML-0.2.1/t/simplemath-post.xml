<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<!DOCTYPE document PUBLIC "-//NIST LaTeXML//LaTeXML article + MathML" "http://dlmf.nist.gov/LaTeXML/LaTeXML-MathML.dtd">
<document xmlns="http://dlmf.nist.gov/LaTeXML" xmlns:m="http://www.w3.org/1998/Math/MathML">
  <section>
    <p>Inline math <Math mode="inline" tex="a+b" text="a + b"><XMath><XMApp><XMTok role="ADDOP">+</XMTok><XMTok font="italic" role="ID">a</XMTok><XMTok font="italic" role="ID">b</XMTok></XMApp></XMath><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mi>a</mi><mo>+</mo><mi>b</mi></mrow></math></Math>.
</p>
    <equation>
      <Math mode="display" tex="a+b+123" text="a + b + 123">
        <XMath>
          <XMApp>
            <XMTok role="ADDOP">+</XMTok>
            <XMTok font="italic" role="ID">a</XMTok>
            <XMTok font="italic" role="ID">b</XMTok>
            <XMTok name="123" role="NUMBER">123</XMTok>
          </XMApp>
        </XMath>
        <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
          <mrow>
            <mi>a</mi>
            <mo>+</mo>
            <mi>b</mi>
            <mo>+</mo>
            <mn>123</mn>
          </mrow>
        </math>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="a+b+f(123)" text="a + b + f@(123)">
        <XMath>
          <XMApp>
            <XMTok role="ADDOP">+</XMTok>
            <XMTok font="italic" role="ID">a</XMTok>
            <XMTok font="italic" role="ID">b</XMTok>
            <XMApp>
              <XMTok font="italic" role="FUNCTION" argclose=")" argopen="(">f</XMTok>
              <XMTok name="123" role="NUMBER">123</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
        <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
          <mrow>
            <mi>a</mi>
            <mo>+</mo>
            <mi>b</mi>
            <mo>+</mo>
            <mrow>
              <mi>f</mi>
              <mo>⁡</mo>
              <mrow>
                <mo>(</mo>
                <mn>123</mn>
                <mo>)</mo>
              </mrow>
            </mrow>
          </mrow>
        </math>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="f_{D}f(a+b)" text="(f _ D)@(f@(a + b))">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok name="Subscript" role="SUBSCRIPT"/>
              <XMTok font="italic" role="DIFFOP">f</XMTok>
              <XMTok font="italic" role="ID">D</XMTok>
            </XMApp>
            <XMApp>
              <XMTok font="italic" role="FUNCTION" argclose=")" argopen="(">f</XMTok>
              <XMApp>
                <XMTok role="ADDOP">+</XMTok>
                <XMTok font="italic" role="ID">a</XMTok>
                <XMTok font="italic" role="ID">b</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
        <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
          <mrow>
            <msub>
              <mi>f</mi>
              <mi>D</mi>
            </msub>
            <mo>⁡</mo>
            <mrow>
              <mi>f</mi>
              <mo>⁡</mo>
              <mrow>
                <mo>(</mo>
                <mi>a</mi>
                <mo>+</mo>
                <mi>b</mi>
                <mo>)</mo>
              </mrow>
            </mrow>
          </mrow>
        </math>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="f_{1}(a+b)" text="f _ 1 * (a + b)">
        <XMath>
          <XMApp>
            <XMTok role="MULOP">⁢</XMTok>
            <XMApp>
              <XMTok name="Subscript" role="SUBSCRIPT"/>
              <XMTok font="italic" role="ID">f</XMTok>
              <XMTok role="NUMBER">1</XMTok>
            </XMApp>
            <XMApp open="(" close=")">
              <XMTok role="ADDOP">+</XMTok>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMTok font="italic" role="ID">b</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
        <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
          <mrow>
            <msub>
              <mi>f</mi>
              <mn>1</mn>
            </msub>
            <mo>⁢</mo>
            <mrow>
              <mo>(</mo>
              <mi>a</mi>
              <mo>+</mo>
              <mi>b</mi>
              <mo>)</mo>
            </mrow>
          </mrow>
        </math>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="\hat{f}a" text="hat@(f) * a">
        <XMath>
          <XMApp>
            <XMTok role="MULOP">⁢</XMTok>
            <XMApp>
              <XMTok name="hat" role="OVERACCENT">^</XMTok>
              <XMTok font="italic" role="FUNCTION">f</XMTok>
            </XMApp>
            <XMTok font="italic" role="ID">a</XMTok>
          </XMApp>
        </XMath>
        <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
          <mrow>
            <mover accent="true">
              <mi>f</mi>
              <mi mathvariant="normal">^</mi>
            </mover>
            <mo>⁢</mo>
            <mi>a</mi>
          </mrow>
        </math>
      </Math>
    </equation>
  </section>
  <section label="sec:restricted" refnum="1">
    <title>Restricted</title>
    <equation>
      <Math mode="display" tex="a(x)+b(x)+123" text="a@(x) + b@(x) + 123">
        <XMath>
          <XMApp>
            <XMTok role="ADDOP">+</XMTok>
            <XMApp>
              <XMTok font="italic" role="FUNCTION" argclose=")" argopen="(">a</XMTok>
              <XMTok font="italic" role="ID">x</XMTok>
            </XMApp>
            <XMApp>
              <XMTok font="italic" role="FUNCTION" argclose=")" argopen="(">b</XMTok>
              <XMTok font="italic" role="ID">x</XMTok>
            </XMApp>
            <XMTok name="123" role="NUMBER">123</XMTok>
          </XMApp>
        </XMath>
        <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
          <mrow>
            <mrow>
              <mi>a</mi>
              <mo>⁡</mo>
              <mrow>
                <mo>(</mo>
                <mi>x</mi>
                <mo>)</mo>
              </mrow>
            </mrow>
            <mo>+</mo>
            <mrow>
              <mi>b</mi>
              <mo>⁡</mo>
              <mrow>
                <mo>(</mo>
                <mi>x</mi>
                <mo>)</mo>
              </mrow>
            </mrow>
            <mo>+</mo>
            <mn>123</mn>
          </mrow>
        </math>
      </Math>
    </equation>
  </section>
</document>
