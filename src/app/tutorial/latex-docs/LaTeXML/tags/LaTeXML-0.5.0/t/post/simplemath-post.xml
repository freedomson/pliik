<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<!DOCTYPE document PUBLIC "-//NIST LaTeXML//LaTeXML article + MathML" "http://dlmf.nist.gov/LaTeXML/LaTeXML-MathML.dtd">
<document xmlns="http://dlmf.nist.gov/LaTeXML" xmlns:m="http://www.w3.org/1998/Math/MathML">
  <section>
    <para>
      <p>Inline math <Math mode="inline" tex="a+b" text="a + b"><XMath><XMApp><XMTok role="ADDOP">+</XMTok><XMTok role="ID" font="italic">a</XMTok><XMTok role="ID" font="italic">b</XMTok></XMApp></XMath><m:math display="inline"><m:mrow><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi></m:mrow></m:math></Math>.
</p>
    </para>
    <para>
      <equation>
        <Math mode="display" tex="a+b+123" text="a + b + 123">
          <XMath>
            <XMApp>
              <XMTok role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok name="123" role="NUMBER">123</XMTok>
            </XMApp>
          </XMath>
          <m:math display="block">
            <m:mrow>
              <m:mi>a</m:mi>
              <m:mo>+</m:mo>
              <m:mi>b</m:mi>
              <m:mo>+</m:mo>
              <m:mn>123</m:mn>
            </m:mrow>
          </m:math>
        </Math>
      </equation>
      <equation>
        <Math mode="display" tex="a+b+f(123)" text="a + b + f@(123)">
          <XMath>
            <XMApp>
              <XMTok role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMApp>
                <XMTok role="FUNCTION" argclose=")" argopen="(" font="italic">f</XMTok>
                <XMTok name="123" role="NUMBER">123</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
          <m:math display="block">
            <m:mrow>
              <m:mi>a</m:mi>
              <m:mo>+</m:mo>
              <m:mi>b</m:mi>
              <m:mo>+</m:mo>
              <m:mrow>
                <m:mi>f</m:mi>
                <m:mo>⁡</m:mo>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mn>123</m:mn>
                  <m:mo>)</m:mo>
                </m:mrow>
              </m:mrow>
            </m:mrow>
          </m:math>
        </Math>
      </equation>
      <equation>
        <Math mode="display" tex="f_{D}f(a+b)" text="f _ D * f@(a + b)">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok name="Subscript" role="SUBSCRIPT"/>
                <XMTok role="ID" font="italic">f</XMTok>
                <XMTok role="ID" font="italic">D</XMTok>
              </XMApp>
              <XMApp>
                <XMTok role="FUNCTION" argclose=")" argopen="(" font="italic">f</XMTok>
                <XMApp>
                  <XMTok role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
          <m:math display="block">
            <m:mrow>
              <m:msub>
                <m:mi>f</m:mi>
                <m:mi>D</m:mi>
              </m:msub>
              <m:mo>⁢</m:mo>
              <m:mrow>
                <m:mi>f</m:mi>
                <m:mo>⁡</m:mo>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>a</m:mi>
                  <m:mo>+</m:mo>
                  <m:mi>b</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
              </m:mrow>
            </m:mrow>
          </m:math>
        </Math>
      </equation>
      <equation>
        <Math mode="display" tex="f_{1}(a+b)" text="f _ 1 * (a + b)">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok name="Subscript" role="SUBSCRIPT"/>
                <XMTok role="ID" font="italic">f</XMTok>
                <XMTok role="NUMBER">1</XMTok>
              </XMApp>
              <XMApp open="(" close=")">
                <XMTok role="ADDOP">+</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
          <m:math display="block">
            <m:mrow>
              <m:msub>
                <m:mi>f</m:mi>
                <m:mn>1</m:mn>
              </m:msub>
              <m:mo>⁢</m:mo>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>a</m:mi>
                <m:mo>+</m:mo>
                <m:mi>b</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
            </m:mrow>
          </m:math>
        </Math>
      </equation>
      <equation>
        <Math mode="display" tex="\hat{f}a" text="hat@(f) * a">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">⁢</XMTok>
              <XMApp role="ID">
                <XMTok name="hat" role="OVERACCENT">^</XMTok>
                <XMTok role="FUNCTION" font="italic">f</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">a</XMTok>
            </XMApp>
          </XMath>
          <m:math display="block">
            <m:mrow>
              <m:mover accent="true">
                <m:mi>f</m:mi>
                <m:mo>^</m:mo>
              </m:mover>
              <m:mo>⁢</m:mo>
              <m:mi>a</m:mi>
            </m:mrow>
          </m:math>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="1" label="sec:restricted">
    <title>Restricted</title>
    <para>
      <equation>
        <Math mode="display" tex="a(x)+b(x)+123" text="a@(x) + b@(x) + 123">
          <XMath>
            <XMApp>
              <XMTok role="ADDOP">+</XMTok>
              <XMApp>
                <XMTok role="FUNCTION" argclose=")" argopen="(" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">x</XMTok>
              </XMApp>
              <XMApp>
                <XMTok role="FUNCTION" argclose=")" argopen="(" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">x</XMTok>
              </XMApp>
              <XMTok name="123" role="NUMBER">123</XMTok>
            </XMApp>
          </XMath>
          <m:math display="block">
            <m:mrow>
              <m:mrow>
                <m:mi>a</m:mi>
                <m:mo>⁡</m:mo>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>x</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
              </m:mrow>
              <m:mo>+</m:mo>
              <m:mrow>
                <m:mi>b</m:mi>
                <m:mo>⁡</m:mo>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>x</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
              </m:mrow>
              <m:mo>+</m:mo>
              <m:mn>123</m:mn>
            </m:mrow>
          </m:math>
        </Math>
      </equation>
    </para>
  </section>
</document>
