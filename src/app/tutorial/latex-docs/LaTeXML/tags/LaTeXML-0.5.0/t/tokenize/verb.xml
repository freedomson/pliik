<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section id="S1" refnum="1">
    <title>Testing Verbatim</title>
    <para id="S1.p1">
      <p>Normal <text font="bold">Bold</text> stuff.</p>
    </para>
    <para id="S1.p2">
      <verbatim font="typewriter">
AbNormal {\bf NonBold} stuff.
</verbatim>
    </para>
    <para id="S1.p3">
      <verbatim font="typewriter"> xxx
AbNormal {\bf NonBold} stuff.
xxx    
</verbatim>
      <p>yyy
More Normal <text font="bold">Bold</text> stuff.</p>
    </para>
    <para id="S1.p4">
      <p>Some inline <verbatim font="typewriter">\verb</verbatim> verbatim.</p>
    </para>
    <para id="S1.p5">
      <p><text>More inline <verbatim font="typewriter">\verb</verbatim> verbatim.</text></p>
    </para>
    <para id="S1.p6">
      <p>x<text width="144.54pt"><text font="bold">x<verbatim font="typewriter">\verb</verbatim>y.</text></text>y</p>
    </para>
  </section>
