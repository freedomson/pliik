<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document PUBLIC "-//NIST LaTeXML//LaTeXML article" "LaTeXML.dtd">
<?latexml class="article"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section>
    <para id="p1">
      <equation id="Ex1">
        <Math mode="display" tex="abc\not=\mathrm{abc}\not=\mathit{abc}\ldots" text="a * b * c not-= abc not-= abc * ldots">
          <XMath>
            <XMApp>
              <XMTok name="MultiRelation"/>
              <XMApp>
                <XMTok role="MULOP">⁢</XMTok>
                <XMTok role="UNKNOWN" font="italic">a</XMTok>
                <XMTok role="UNKNOWN" font="italic">b</XMTok>
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="RELOP" name="not-=">≠</XMTok>
              <XMTok role="UNKNOWN">abc</XMTok>
              <XMTok role="RELOP" name="not-=">≠</XMTok>
              <XMApp>
                <XMTok role="MULOP">⁢</XMTok>
                <XMTok role="UNKNOWN" font="italic">abc</XMTok>
                <XMTok name="ldots" role="ID">…</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex2">
        <Math mode="display" tex="\pi\approx 3.1415\; 926" text="pi approx 3.1415926">
          <XMath>
            <XMApp>
              <XMTok name="approx" role="RELOP">≈</XMTok>
              <XMTok name="pi" role="UNKNOWN" font="italic">π</XMTok>
              <XMTok role="NUMBER" name="3.1415926">3.1415 926</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex3">
        <Math mode="display" tex="a\cdot b\cdot c\cdot\cdot\cdot" text="(a cdot b cdot c) * cdots">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok name="cdot" role="MULOP">⋅</XMTok>
                <XMTok role="UNKNOWN" font="italic">a</XMTok>
                <XMTok role="UNKNOWN" font="italic">b</XMTok>
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMApp>
              <XMTok name="cdots" role="ID">⋯</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
</document>
