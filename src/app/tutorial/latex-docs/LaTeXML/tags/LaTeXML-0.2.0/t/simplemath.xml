<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document PUBLIC "-//NIST LaTeXML//LaTeXML article" "LaTeXML.dtd">
<?latexml class="article"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section>
    <p>Inline math <Math mode="inline" tex="a+b"><XMath><XMTok font="italic">a</XMTok><XMTok role="ADDOP">+</XMTok><XMTok font="italic">b</XMTok></XMath></Math>.
</p>
    <equation>
      <Math mode="display" tex="a+b+123">
        <XMath>
          <XMTok font="italic">a</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok font="italic">b</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok name="123" role="NUMBER">123</XMTok>
        </XMath>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="a+b+f(123)">
        <XMath>
          <XMTok font="italic">a</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok font="italic">b</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok font="italic">f</XMTok>
          <XMTok role="OPEN">(</XMTok>
          <XMTok name="123" role="NUMBER">123</XMTok>
          <XMTok role="CLOSE">)</XMTok>
        </XMath>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="f_{D}f(a+b)">
        <XMath>
          <XMTok font="italic">f</XMTok>
          <XMApp role="POSTSUBSCRIPT">
            <XMArg rule="Subscript">
              <XMTok font="italic">D</XMTok>
            </XMArg>
          </XMApp>
          <XMTok font="italic">f</XMTok>
          <XMTok role="OPEN">(</XMTok>
          <XMTok font="italic">a</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok font="italic">b</XMTok>
          <XMTok role="CLOSE">)</XMTok>
        </XMath>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="f_{1}(a+b)">
        <XMath>
          <XMTok font="italic">f</XMTok>
          <XMApp role="POSTSUBSCRIPT">
            <XMArg rule="Subscript">
              <XMTok role="NUMBER">1</XMTok>
            </XMArg>
          </XMApp>
          <XMTok role="OPEN">(</XMTok>
          <XMTok font="italic">a</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok font="italic">b</XMTok>
          <XMTok role="CLOSE">)</XMTok>
        </XMath>
      </Math>
    </equation>
    <equation>
      <Math mode="display" tex="\hat{f}a">
        <XMath>
          <XMApp>
            <XMTok name="hat" role="OVERACCENT">^</XMTok>
            <XMArg>
              <XMTok font="italic">f</XMTok>
            </XMArg>
          </XMApp>
          <XMTok font="italic">a</XMTok>
        </XMath>
      </Math>
    </equation>
  </section>
  <section label="sec:restricted" refnum="1">
    <title>Restricted</title>
    <equation>
      <Math mode="display" tex="a(x)+b(x)+123">
        <XMath>
          <XMTok font="italic">a</XMTok>
          <XMTok role="OPEN">(</XMTok>
          <XMTok font="italic">x</XMTok>
          <XMTok role="CLOSE">)</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok font="italic">b</XMTok>
          <XMTok role="OPEN">(</XMTok>
          <XMTok font="italic">x</XMTok>
          <XMTok role="CLOSE">)</XMTok>
          <XMTok role="ADDOP">+</XMTok>
          <XMTok name="123" role="NUMBER">123</XMTok>
        </XMath>
      </Math>
    </equation>
  </section>
</document>
