<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section refnum="1" xml:id="S1">
    <title><tag>1 </tag>Basic math</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="((a+b)-c)" xml:id="S1.Ex1.m1" text="(a + b) - c">
          <XMath>
            <XMApp open="(" close=")">
              <XMTok meaning="minus" role="ADDOP">-</XMTok>
              <XMApp open="(" close=")">
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">c</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="(a+(b-c))" xml:id="S1.Ex2.m1" text="a + (b - c)">
          <XMath>
            <XMApp open="(" close=")">
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp open="(" close=")">
                <XMTok meaning="minus" role="ADDOP">-</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag>2 </tag>Intervals</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex3">
        <Math mode="display" tex="(a,b)" xml:id="S2.Ex3.m1" text="open-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok meaning="open-interval" role="FENCED" argclose=")" argopen="(" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex4">
        <Math mode="display" tex="[a,b)" xml:id="S2.Ex4.m1" text="closed-open-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok meaning="closed-open-interval" role="FENCED" argclose=")" argopen="[" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex5">
        <Math mode="display" tex="(a,b]" xml:id="S2.Ex5.m1" text="open-closed-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok meaning="open-closed-interval" role="FENCED" argclose="]" argopen="(" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex6">
        <Math mode="display" tex="[a,b]" xml:id="S2.Ex6.m1" text="closed-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok meaning="closed-interval" role="FENCED" argclose="]" argopen="[" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag>3 </tag>Cases</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex7">
        <Math mode="display" tex="\left.\begin{array}[]{l}a\\&#10;b\end{array}\right\}=\left\{\begin{array}[]{l}c\\&#10;d\end{array}\right." xml:id="S3.Ex7.m1" text="cases[[a], [b]] = cases[[c], [d]]">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMArray meaning="cases" role="ARRAY" close="}">
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">a</XMTok>
                  </XMCell>
                </XMRow>
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">b</XMTok>
                  </XMCell>
                </XMRow>
              </XMArray>
              <XMArray meaning="cases" role="ARRAY" open="{">
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">c</XMTok>
                  </XMCell>
                </XMRow>
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">d</XMTok>
                  </XMCell>
                </XMRow>
              </XMArray>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S3.p2">
      <equation xml:id="S3.Ex8">
        <Math mode="display" tex="\left(\begin{array}[]{l}c\\&#10;d\end{array}\right)" xml:id="S3.Ex8.m1" text="Array[[c], [d]]">
          <XMath>
            <XMArray role="ARRAY" open="(" close=")">
              <XMRow>
                <XMCell align="left">
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMCell>
              </XMRow>
              <XMRow>
                <XMCell align="left">
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMCell>
              </XMRow>
            </XMArray>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex9">
        <Math mode="display" tex="(a+\left(\begin{array}[]{l}c\\&#10;d\end{array}\right)+b)" xml:id="S3.Ex9.m1" text="a + Array[[c], [d]] + b">
          <XMath>
            <XMApp open="(" close=")">
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMArray role="ARRAY" open="(" close=")">
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">c</XMTok>
                  </XMCell>
                </XMRow>
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">d</XMTok>
                  </XMCell>
                </XMRow>
              </XMArray>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="4" xml:id="S4">
    <title><tag>4 </tag>Atop</title>
    <para xml:id="S4.p1">
      <equation xml:id="S4.Ex10">
        <Math mode="display" tex="a+{b\atop c}+d" xml:id="S4.Ex10.m1" text="a + STACKED@(b, c) + d">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok role="STACKED" style="display"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">d</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S4.Ex11">
        <Math mode="display" tex="a+\left[b\atop c\right]+d" xml:id="S4.Ex11.m1" text="a + STACKED@(b, c) + d">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp open="[" close="]">
                <XMTok role="STACKED" style="display"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">d</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="5" xml:id="S5">
    <title><tag>5 </tag>Delimiters</title>
    <para xml:id="S5.p1">
      <equation xml:id="S5.Ex12">
        <Math mode="display" tex="\left({a\over b}\right)" xml:id="S5.Ex12.m1" text="a / b">
          <XMath>
            <XMApp open="(" close=")">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex13">
        <Math mode="display" tex="\left[{a\over b}\right]" xml:id="S5.Ex13.m1" text="a / b">
          <XMath>
            <XMApp open="[" close="]">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex14">
        <Math mode="display" tex="\left[{a\over b}\right]" xml:id="S5.Ex14.m1" text="a / b">
          <XMath>
            <XMApp open="[" close="]">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex15">
        <Math mode="display" tex="\left\{{a\over b}\right\}" xml:id="S5.Ex15.m1" text="set@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="set" role="FENCED" argclose="}" argopen="{"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex16">
        <Math mode="display" tex="\left\{{a\over b}\right\}" xml:id="S5.Ex16.m1" text="set@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="set" role="FENCED" argclose="}" argopen="{"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex17">
        <Math mode="display" tex="\left\lfloor{a\over b}\right\rfloor" xml:id="S5.Ex17.m1" text="floor@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="floor" role="FENCED" argclose="⌋" argopen="⌊"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex18">
        <Math mode="display" tex="\left\lceil{a\over b}\right\rceil" xml:id="S5.Ex18.m1" text="ceiling@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="ceiling" role="FENCED" argclose="⌉" argopen="⌈"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex19">
        <Math mode="display" tex="\left&lt;{a\over b}\right&gt;" xml:id="S5.Ex19.m1" text="a / b">
          <XMath>
            <XMApp open="⟨" close="⟩">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex20">
        <Math mode="display" tex="\left\langle{a\over b}\right\rangle" xml:id="S5.Ex20.m1" text="a / b">
          <XMath>
            <XMApp open="⟨" close="⟩">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex21">
        <Math mode="display" tex="\left|{a\over b}\right|" xml:id="S5.Ex21.m1" text="absolute-value@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="absolute-value" role="FENCED" argclose="|" argopen="|"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex22">
        <Math mode="display" tex="\left|{a\over b}\right|" xml:id="S5.Ex22.m1" text="absolute-value@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="absolute-value" role="FENCED" argclose="|" argopen="|"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex23">
        <Math mode="display" tex="\left\|{a\over b}\right\|" xml:id="S5.Ex23.m1" text="norm@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="norm" role="FENCED" argclose="∥" argopen="∥"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex24">
        <Math mode="display" tex="\left\|{a\over b}\right\|" xml:id="S5.Ex24.m1" text="norm@(a / b)">
          <XMath>
            <XMApp>
              <XMTok meaning="norm" role="FENCED" argclose="∥" argopen="∥"/>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex25">
        <Math mode="display" tex="\left\uparrow{a\over b}\right\downarrow" xml:id="S5.Ex25.m1" text="a / b">
          <XMath>
            <XMApp open="↑" close="↓">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex26">
        <Math mode="display" tex="\left\Uparrow{a\over b}\right\Downarrow" xml:id="S5.Ex26.m1" text="a / b">
          <XMath>
            <XMApp open="⇑" close="⇓">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex27">
        <Math mode="display" tex="\left\updownarrow{a\over b}\right\Updownarrow" xml:id="S5.Ex27.m1" text="a / b">
          <XMath>
            <XMApp open="↕" close="⇕">
              <XMTok meaning="divide" role="MULOP" style="display"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S5.p2">
      <equation xml:id="S5.Ex28">
        <Math mode="display" tex="a+\left\langle{b\over c}\right\rangle+d" xml:id="S5.Ex28.m1" text="a + b / c + d">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp open="⟨" close="⟩">
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">d</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S5.p3">
      <equation xml:id="S5.Ex29">
        <Math mode="display" tex="{a\over b}\left/{c\over d}\right." xml:id="S5.Ex29.m1" text="(a / b) / (c / d)">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">/</XMTok>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="divide" role="MULOP" style="display"/>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
