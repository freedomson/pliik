<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="abc\not=\mathrm{abc}\not=\mathit{abc}\ldots" xml:id="S0.Ex1.m1" text="a * b * c not-equals abc not-equals abc * ldots">
        <XMath>
          <XMApp>
            <XMTok meaning="multirelation"/>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok role="UNKNOWN" font="italic">a</XMTok>
              <XMTok role="UNKNOWN" font="italic">b</XMTok>
              <XMTok role="UNKNOWN" font="italic">c</XMTok>
            </XMApp>
            <XMTok meaning="not-equals" role="RELOP" name="not-=">≠</XMTok>
            <XMTok role="UNKNOWN">abc</XMTok>
            <XMTok meaning="not-equals" role="RELOP" name="not-=">≠</XMTok>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok role="UNKNOWN" font="italic">abc</XMTok>
              <XMTok name="ldots" role="ID">…</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="\pi\approx 3.1415\; 926" xml:id="S0.Ex2.m1" text="pi almost-equals 3.1415926">
        <XMath>
          <XMApp>
            <XMTok meaning="almost-equals" name="approx" role="RELOP">≈</XMTok>
            <XMTok name="pi" role="UNKNOWN" font="italic">π</XMTok>
            <XMTok meaning="3.1415926" role="NUMBER">3.1415 926</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex3">
      <Math mode="display" tex="a\cdot b\cdot c\cdot\cdot\cdot" xml:id="S0.Ex3.m1" text="(a cdot b cdot c) * cdots">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMApp>
              <XMTok name="cdot" role="MULOP">⋅</XMTok>
              <XMTok role="UNKNOWN" font="italic">a</XMTok>
              <XMTok role="UNKNOWN" font="italic">b</XMTok>
              <XMTok role="UNKNOWN" font="italic">c</XMTok>
            </XMApp>
            <XMTok name="cdots" role="ID">⋯</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
</document>
