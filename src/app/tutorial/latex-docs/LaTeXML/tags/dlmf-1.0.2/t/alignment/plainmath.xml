  <para>
    <equation>
      <Math mode="display" tex="s(x)=\cases{+1&amp;\hbox{whatever}\cr-1&amp;\hbox{$x&lt;0$}\cr 0}" text="s * x = cases[[+ 1, [whatever]], [- 1, [lessx0]], [0, ]]">
        <XMath>
          <XMApp>
            <XMTok meaning="equals" role="RELOP">=</XMTok>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok role="UNKNOWN" possibleFunction="yes" font="italic">s</XMTok>
              <XMTok role="UNKNOWN" open="(" close=")" font="italic">x</XMTok>
            </XMApp>
            <XMArray meaning="cases">
              <XMRow>
                <XMCell align="left">
                  <XMApp>
                    <XMTok meaning="plus" role="ADDOP">+</XMTok>
                    <XMTok meaning="1" role="NUMBER">1</XMTok>
                  </XMApp>
                </XMCell>
                <XMCell align="left">
                  <XMText>whatever</XMText>
                </XMCell>
              </XMRow>
              <XMRow>
                <XMCell align="left">
                  <XMApp>
                    <XMTok meaning="minus" role="ADDOP">-</XMTok>
                    <XMTok meaning="1" role="NUMBER">1</XMTok>
                  </XMApp>
                </XMCell>
                <XMCell align="left">
                  <XMText>
                    <Math mode="inline" tex="x&lt;0" text="x less 0">
                      <XMath>
                        <XMApp>
                          <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                          <XMTok role="UNKNOWN" font="italic">x</XMTok>
                          <XMTok meaning="0" role="NUMBER">0</XMTok>
                        </XMApp>
                      </XMath>
                    </Math>
                  </XMText>
                </XMCell>
              </XMRow>
              <XMRow>
                <XMCell align="left">
                  <XMTok meaning="0" role="NUMBER">0</XMTok>
                </XMCell>
                <XMCell/>
              </XMRow>
            </XMArray>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para>
    <equation>
      <Math mode="display" tex="\eqalign{x&amp;\ll y+i+\cdots+y_{n}\cr&amp;\leq z}" text="Array[[x, absent lessless y + i + cdots + y _ n], [, absent less= z]]">
        <XMath>
          <XMArray>
            <XMRow>
              <XMCell align="right">
                <XMTok role="UNKNOWN" font="italic">x</XMTok>
              </XMCell>
              <XMCell align="left">
                <XMApp>
                  <XMTok meaning="much-less-than" name="ll" role="RELOP">≪</XMTok>
                  <XMTok meaning="absent"/>
                  <XMApp>
                    <XMTok meaning="plus" role="ADDOP">+</XMTok>
                    <XMTok role="UNKNOWN" font="italic">y</XMTok>
                    <XMTok role="UNKNOWN" font="italic">i</XMTok>
                    <XMTok name="cdots" role="ID">⋯</XMTok>
                    <XMApp>
                      <XMTok role="SUBSCRIPTOP" scriptpos="post6"/>
                      <XMTok role="UNKNOWN" font="italic">y</XMTok>
                      <XMTok role="UNKNOWN" font="italic">n</XMTok>
                    </XMApp>
                  </XMApp>
                </XMApp>
              </XMCell>
            </XMRow>
            <XMRow>
              <XMCell/>
              <XMCell align="left">
                <XMApp>
                  <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
                  <XMTok meaning="absent"/>
                  <XMTok role="UNKNOWN" font="italic">z</XMTok>
                </XMApp>
              </XMCell>
            </XMRow>
          </XMArray>
        </XMath>
      </Math>
    </equation>
