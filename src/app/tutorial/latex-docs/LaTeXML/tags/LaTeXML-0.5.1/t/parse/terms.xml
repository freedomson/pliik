<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document PUBLIC "-//NIST LaTeXML//LaTeXML article" "LaTeXML.dtd">
<?latexml class="article"?>
<?latexml package="latexml"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section id="S1" refnum="1">
    <title>Basic math</title>
    <para id="S1.p1">
      <equation id="Ex1">
        <Math mode="display" tex="a+b+c-d+e" text="((a + b + c) - d) + e">
          <XMath>
            <XMApp>
              <XMTok role="ADDOP">+</XMTok>
              <XMApp>
                <XMTok role="ADDOP">-</XMTok>
                <XMApp>
                  <XMTok role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex2">
        <Math mode="display" tex="a+bc-de" text="(a + b * c) - d * e">
          <XMath>
            <XMApp>
              <XMTok role="ADDOP">-</XMTok>
              <XMApp>
                <XMTok role="ADDOP">+</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok role="MULOP">⁢</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok role="MULOP">⁢</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex3">
        <Math mode="display" tex="abc/de" text="((a * b * c) / d) * e">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok role="MULOP" style="inline">/</XMTok>
                <XMApp>
                  <XMTok role="MULOP">⁢</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex4">
        <Math mode="display" tex="a\times b\times c/d\times e" text="((a times b times c) / d) times e">
          <XMath>
            <XMApp>
              <XMTok name="times" role="MULOP">×</XMTok>
              <XMApp>
                <XMTok role="MULOP" style="inline">/</XMTok>
                <XMApp>
                  <XMTok name="times" role="MULOP">×</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section id="S2" refnum="2">
    <title>Evaluate At</title>
    <para id="S2.p1">
      <equation id="Ex5">
        <Math mode="display" tex="a|_{\infty}" text="AT@(a, Infinity)">
          <XMath>
            <XMApp>
              <XMTok name="AT"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok name="Infinity" role="ID">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex6">
        <Math mode="display" tex="ab|_{{a=0}}cd|_{{c=0}}e" text="AT@(AT@(a * b, a = 0) * c * d, c = 0) * e">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok name="AT"/>
                <XMApp>
                  <XMTok role="MULOP">⁢</XMTok>
                  <XMApp>
                    <XMTok name="AT"/>
                    <XMApp>
                      <XMTok role="MULOP">⁢</XMTok>
                      <XMTok role="ID" font="italic">a</XMTok>
                      <XMTok role="ID" font="italic">b</XMTok>
                    </XMApp>
                    <XMApp>
                      <XMTok role="RELOP">=</XMTok>
                      <XMTok role="ID" font="italic">a</XMTok>
                      <XMTok role="NUMBER">0</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="NUMBER">0</XMTok>
                </XMApp>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section id="S3" refnum="3">
    <title>Absolute values</title>
    <para id="S3.p1">
      <equation id="Ex7">
        <Math mode="display" tex="|a|" text="Abs@(a)">
          <XMath>
            <XMApp>
              <XMTok name="Abs" role="FENCED" argclose="|" argopen="|"/>
              <XMTok role="ID" font="italic">a</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex8">
        <Math mode="display" tex="||a||" text="Abs@(Abs@(a))">
          <XMath>
            <XMApp>
              <XMTok name="Abs" role="FENCED" argclose="|" argopen="|"/>
              <XMApp>
                <XMTok name="Abs" role="FENCED" argclose="|" argopen="|"/>
                <XMTok role="ID" font="italic">a</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex9">
        <Math mode="display" tex="|a|_{\infty}" text="(Abs@(a)) _ Infinity">
          <XMath>
            <XMApp>
              <XMTok name="Subscript" role="SUBSCRIPT"/>
              <XMApp>
                <XMTok name="Abs" role="FENCED" argclose="|" argopen="|"/>
                <XMTok role="ID" font="italic">a</XMTok>
              </XMApp>
              <XMTok name="Infinity" role="ID">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex10">
        <Math mode="display" tex="|a+b|" text="Abs@(a + b)">
          <XMath>
            <XMApp>
              <XMTok name="Abs" role="FENCED" argclose="|" argopen="|"/>
              <XMApp>
                <XMTok role="ADDOP">+</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex11">
        <Math mode="display" tex="|a-|b||" text="Abs@(a - Abs@(b))">
          <XMath>
            <XMApp>
              <XMTok name="Abs" role="FENCED" argclose="|" argopen="|"/>
              <XMApp>
                <XMTok role="ADDOP">-</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok name="Abs" role="FENCED" argclose="|" argopen="|"/>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section id="S4" refnum="4">
    <title>Failures w/o Backtracking</title>
    <para id="S4.p1">
      <equation id="Ex12">
        <Math mode="display" tex="||a|-|b||">
          <XMath>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ID" font="italic">a</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ADDOP">-</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ID" font="italic">b</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex13">
        <Math mode="display" tex="||a|-b|">
          <XMath>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ID" font="italic">a</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ADDOP">-</XMTok>
            <XMTok role="ID" font="italic">b</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex14">
        <Math mode="display" tex="|a|b|_{\infty}">
          <XMath>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ID" font="italic">a</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ID" font="italic">b</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMApp role="POSTSUBSCRIPT">
              <XMTok name="Infinity" role="ID">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation id="Ex15">
        <Math mode="display" tex="|ab|_{\infty}c|">
          <XMath>
            <XMTok role="VERTBAR">|</XMTok>
            <XMTok role="ID" font="italic">a</XMTok>
            <XMTok role="ID" font="italic">b</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
            <XMApp role="POSTSUBSCRIPT">
              <XMTok name="Infinity" role="ID">∞</XMTok>
            </XMApp>
            <XMTok role="ID" font="italic">c</XMTok>
            <XMTok role="VERTBAR">|</XMTok>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
</document>
