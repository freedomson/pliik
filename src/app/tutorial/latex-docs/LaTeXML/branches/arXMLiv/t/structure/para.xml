<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section refnum="1" xml:id="S1">
    <title><tag>1 </tag>Display Math</title>
    <para xml:id="S1.p1">
      <p>Begin first paragraph,</p>
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="a+b," xml:id="S1.Ex1.m1" text="a + b">
          <XMath>
            <XMApp punctuation=",">
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" font="italic">a</XMTok>
              <XMTok role="UNKNOWN" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <p>continuing paragraph.</p>
    </para>
    <para xml:id="S1.p2">
      <p>Single line paragraph.</p>
    </para>
    <para xml:id="S1.p3">
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="a+b." xml:id="S1.Ex2.m1" text="a + b">
          <XMath>
            <XMApp punctuation=".">
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" font="italic">a</XMTok>
              <XMTok role="UNKNOWN" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S1.p4">
      <p>Another single line paragraph.</p>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag>2 </tag>Itemizations</title>
    <para xml:id="S2.p1">
      <p>Begin first paragraph,</p>
      <itemize xml:id="I1">
        <item xml:id="I1.i1">
          <para xml:id="I1.i1.p1">
            <p>A</p>
          </para>
        </item>
        <item xml:id="I1.i2">
          <para xml:id="I1.i2.p1">
            <p>B</p>
          </para>
        </item>
      </itemize>
      <p>continuing paragraph.</p>
    </para>
    <para xml:id="S2.p2">
      <p>Single line paragraph.</p>
    </para>
    <para xml:id="S2.p3">
      <itemize xml:id="I2">
        <item xml:id="I2.i1">
          <para xml:id="I2.i1.p1">
            <p>A</p>
          </para>
        </item>
        <item xml:id="I2.i2">
          <para xml:id="I2.i2.p1">
            <p>B</p>
          </para>
        </item>
      </itemize>
    </para>
    <para xml:id="S2.p4">
      <p>Another single line paragraph.</p>
    </para>
    <para xml:id="S2.p5">
      <p>Multiple Para within item</p>
      <itemize xml:id="I3">
        <item xml:id="I3.i1">
          <para xml:id="I3.i1.p1">
            <p>A</p>
          </para>
          <para xml:id="I3.i1.p2">
            <p>B</p>
          </para>
        </item>
        <item xml:id="I3.i2">
          <para xml:id="I3.i2.p1">
            <p>C</p>
          </para>
        </item>
      </itemize>
    </para>
  </section>
</document>
