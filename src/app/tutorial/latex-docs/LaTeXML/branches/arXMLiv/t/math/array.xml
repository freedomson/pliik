<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <para xml:id="p1">
    <p>An Array</p>
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="\begin{array}[]{ccc}a&amp;b&amp;c\\&#10;c&amp;&amp;d\\&#10;\\&#10;e&amp;f\\&#10;\end{array}" xml:id="S0.Ex1.m1" text="Array[[a, b, c], [c, , d], [, , ], [e, f, ]]">
        <XMath>
          <XMArray role="ARRAY">
            <XMRow>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">a</XMTok>
              </XMCell>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">b</XMTok>
              </XMCell>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMCell>
            </XMRow>
            <XMRow>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMCell>
              <XMCell/>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">d</XMTok>
              </XMCell>
            </XMRow>
            <XMRow>
              <XMCell/>
              <XMCell/>
              <XMCell/>
            </XMRow>
            <XMRow>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">e</XMTok>
              </XMCell>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">f</XMTok>
              </XMCell>
              <XMCell/>
            </XMRow>
          </XMArray>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="\matrix{a&amp;b\cr c&amp;d}" xml:id="S0.Ex2.m1" text="Matrix[[a, b], [c, d]]">
        <XMath>
          <XMArray name="Matrix">
            <XMRow>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">a</XMTok>
              </XMCell>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">b</XMTok>
              </XMCell>
            </XMRow>
            <XMRow>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMCell>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">d</XMTok>
              </XMCell>
            </XMRow>
          </XMArray>
        </XMath>
      </Math>
    </equation>
    <p>Done.</p>
  </para>
