<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section refnum="1" xml:id="S1">
    <title><tag>1 </tag>Grouping in multi-relations</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="a=b=c=d=e" xml:id="S1.Ex1.m1" text="a = b = c = d = e">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="a=b,c=d" xml:id="S1.Ex2.m1" text="formulae@(a = b, c = d)">
          <XMath>
            <XMApp>
              <XMTok meaning="formulae" separators=","/>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" tex="a=b,c,d,e" xml:id="S1.Ex3.m1" text="a = list@(b, c, d, e)">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok meaning="list" role="FENCED" separators=",,,"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex4">
        <Math mode="display" tex="a=b,c,d=e" xml:id="S1.Ex4.m1" text="formulae@(a = list@(b, c), d = e)">
          <XMath>
            <XMApp>
              <XMTok meaning="formulae" separators=","/>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok meaning="list" role="FENCED" separators=","/>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex5">
        <Math mode="display" tex="a=b&gt;c\leq d=e" xml:id="S1.Ex5.m1" text="a = b &gt; c less= d = e">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok meaning="greater-than" role="RELOP">&gt;</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <paragraph xml:id="S1.SS0.SSS0.P1">
      <title>But period is somewhat different!</title>
      <para xml:id="S1.SS0.SSS0.P1.p1">
        <equation xml:id="S1.Ex6">
          <Math mode="display" tex="a=b,c.d=e" xml:id="S1.Ex6.m1" text="formulae@(a = list@(b, c), d = e)">
            <XMath>
              <XMApp>
                <XMTok meaning="formulae" separators="."/>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMApp>
                    <XMTok meaning="list" role="FENCED" separators=","/>
                    <XMTok role="ID" font="italic">b</XMTok>
                    <XMTok role="ID" font="italic">c</XMTok>
                  </XMApp>
                </XMApp>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex7">
          <Math mode="display" tex="a=b.c,d=e" xml:id="S1.Ex7.m1" text="formulae@(a = b, list@(c, d) = e)">
            <XMath>
              <XMApp>
                <XMTok meaning="formulae" separators="."/>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMApp>
                    <XMTok meaning="list" role="FENCED" separators=","/>
                    <XMTok role="ID" font="italic">c</XMTok>
                    <XMTok role="ID" font="italic">d</XMTok>
                  </XMApp>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
    </paragraph>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag>2 </tag>Arrows as meta-relation</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex8">
        <Math mode="display" tex="a=b\rightarrow c\leq d=e" xml:id="S2.Ex8.m1" text="a = b rightarrow c less= d = e">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok name="rightarrow" role="ARROW">→</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex9">
        <Math mode="display" tex="a=b,c\rightarrow d=e" xml:id="S2.Ex9.m1" text="formulae@(a = b, c rightarrow d = e)">
          <XMath>
            <XMApp>
              <XMTok meaning="formulae" separators=","/>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="multirelation"/>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok name="rightarrow" role="ARROW">→</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex10">
        <Math mode="display" tex="a=b\leftrightarrow c\leq d=e" xml:id="S2.Ex10.m1" text="a = b leftrightarrow c less= d = e">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="multirelation"/>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex11">
        <Math mode="display" tex="a=b,c\leftrightarrow d=e" xml:id="S2.Ex11.m1" text="a = list@(b, c) leftrightarrow d = e">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok meaning="list" role="FENCED" separators=","/>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag>3 </tag>Other Meta relations</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex12">
        <Math mode="display" tex="a(&lt;e)" xml:id="S3.Ex12.m1" text="annotated@(a, absent less e)">
          <XMath>
            <XMApp>
              <XMTok meaning="annotated"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp open="(" close=")">
                <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                <XMTok meaning="absent"/>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex13">
        <Math mode="display" tex="a;(&lt;e)" xml:id="S3.Ex13.m1" text="annotated@(a, absent less e)">
          <XMath>
            <XMApp>
              <XMTok meaning="annotated"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp open="(" close=")">
                <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                <XMTok meaning="absent"/>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex14">
        <Math mode="display" tex="a=b+c+d(&lt;e)" xml:id="S3.Ex14.m1" text="a = annotated@(b + c + d, absent less e)">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok meaning="annotated"/>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMApp open="(" close=")">
                  <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                  <XMTok meaning="absent"/>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex15">
        <Math mode="display" tex="a=b+c+d\mod e" xml:id="S3.Ex15.m1" text="a = modulo@(b + c + d, e)">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok meaning="modulo" role="MODIFIEROP">mod</XMTok>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
</document>
