<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="\{ a\}" xml:id="S0.Ex1.m1" text="set@(a)">
        <XMath>
          <XMApp>
            <XMTok meaning="set" role="FENCED" argclose="}" argopen="{"/>
            <XMTok role="ID" font="italic">a</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="\{ a:a\in b\}" xml:id="S0.Ex2.m1" text="conditional-set@(a, a element-of b)">
        <XMath>
          <XMApp>
            <XMTok meaning="conditional-set" role="FENCED" argclose="}" argopen="{" separators=":"/>
            <XMTok role="ID" font="italic">a</XMTok>
            <XMApp>
              <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex3">
      <Math mode="display" tex="\{ a|a\in b\}" xml:id="S0.Ex3.m1" text="conditional-set@(a, a element-of b)">
        <XMath>
          <XMApp>
            <XMTok meaning="conditional-set" role="FENCED" argclose="}" argopen="{" separators="|"/>
            <XMTok role="ID" font="italic">a</XMTok>
            <XMApp>
              <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex4">
      <Math mode="display" tex="\{ a|a\in b\} _{{c=0}}" xml:id="S0.Ex4.m1" text="(conditional-set@(a, a element-of b)) _ (c = 0)">
        <XMath>
          <XMApp>
            <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
            <XMApp>
              <XMTok meaning="conditional-set" role="FENCED" argclose="}" argopen="{" separators="|"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="0" role="NUMBER">0</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
</document>
