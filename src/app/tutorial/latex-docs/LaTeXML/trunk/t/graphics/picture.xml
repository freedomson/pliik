    <paragraph xml:id="S2.SS0.SSS0.P2">
      <title>Vectors:</title>
      <para xml:id="S2.SS0.SSS0.P2.p1">
        <picture fill="black" height="100.0pt" stroke="black" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.P2.p1.pic1" tex="\begin{picture}(100.0,100.0)(-100.0,-100.0)\put(0.0,0.0){\vector(-1,0){100.0}}\put(0.0,0.0){\vector(-1,-1){100.0}}\put(0.0,0.0){\vector(0,-1){100.0}}\end{picture}">
          <g transform="translate(100,100)">
            <g transform="translate(0,0)">
              <line points="0,0 -100,0" stroke-width="0.4" terminators="-&gt;"/>
            </g>
            <g transform="translate(0,0)">
              <line points="0,0 -100,-100" stroke-width="0.4" terminators="-&gt;"/>
            </g>
            <g transform="translate(0,0)">
              <line points="0,0 0,-100" stroke-width="0.4" terminators="-&gt;"/>
            </g>
          </g>
        </picture>
      </para>
    </paragraph>
    <paragraph xml:id="S2.SS0.SSS0.P3">
      <title>Boxes;</title>
      <para xml:id="S2.SS0.SSS0.P3.p1">
        <picture fill="black" height="100.0pt" stroke="black" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.P3.p1.pic1" tex="\begin{picture}(100.0,100.0)\put(0.0,0.0){\bf A}\put(15.0,15.0){\framebox(15.0,15.0){A}}\put(30.0,30.0){\framebox(15.0,15.0){\bf A}}\put(45.0,45.0){\framebox(15.0,15.0){A}}\put(60.0,20.0){\frame{\shortstack{A\\B\\\bf C\\D}}}\put(75.0,20.0){\framebox(15.0,80.0){\shortstack{A\\\bf B\\C\\D}}}\end{picture}">
          <g transform="translate(0,0)">
            <text font="bold">A</text>
          </g>
          <g transform="translate(15,15)">
            <rect fill="none" height="15" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="15" width="15">
              <text>A</text>
            </g>
          </g>
          <g transform="translate(30,30)">
            <rect fill="none" height="15" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="15" width="15">
              <text font="bold">A</text>
            </g>
          </g>
          <g transform="translate(45,45)">
            <rect fill="none" height="15" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="15" width="15">
              <text>A</text>
            </g>
          </g>
          <g transform="translate(60,20)">
            <g framed="true" stroke-width="0.4">
              <inline-block>
                <p>A</p>
                <p>B</p>
                <p>
                  <text font="bold">C</text>
                </p>
                <p>D</p>
              </inline-block>
            </g>
          </g>
          <g transform="translate(75,20)">
            <rect fill="none" height="80" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="80" width="15">
              <inline-block>
                <p>A</p>
                <p>
                  <text font="bold">B</text>
                </p>
                <p>C</p>
                <p>D</p>
              </inline-block>
            </g>
          </g>
        </picture>
      </para>
    </paragraph>
    <paragraph xml:id="S2.SS0.SSS0.P4">
      <title>Circles:</title>
      <para xml:id="S2.SS0.SSS0.P4.p1">
        <picture fill="black" height="100.0pt" stroke="black" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.P4.p1.pic1" tex="\begin{picture}(100.0,100.0)\put(30.0,30.0){\circle{30.0}\vector(0,1){15.0}\circle*{5.0}}\put(70.0,70.0){\oval(60.0,30.0)}\end{picture}">
          <g transform="translate(30,30)">
            <circle fill="none" r="15.2" stroke-width="0.4" x="0" y="0"/>
            <line points="0,0 0,15" stroke-width="0.4" terminators="-&gt;"/>
            <circle r="2.5" stroke="none" x="0" y="0"/>
          </g>
          <g transform="translate(70,70)">
            <rect fill="none" height="30" rx="15" stroke-width="0.4" width="60" x="-30" y="-15"/>
          </g>
        </picture>
      </para>
    </paragraph>
    <paragraph xml:id="S2.SS0.SSS0.P5">
      <title>Curves:</title>
      <para xml:id="S2.SS0.SSS0.P5.p1">
        <picture fill="black" height="100.0pt" stroke="black" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.P5.p1.pic1" tex="\begin{picture}(100.0,100.0)\qbezier(40.0,80.0)(80.0,120.0)(100.0,80.0)\qbezier[50](40.0,89.0)(80.0,129.0)(100.0,89.0)\end{picture}">
          <bezier points="40,80 80,120 100,80" stroke-width="0.4"/>
          <bezier displayedpoints="50" points="40,89 80,129 100,89" stroke-width="0.4"/>
        </picture>
      </para>
    </paragraph>
