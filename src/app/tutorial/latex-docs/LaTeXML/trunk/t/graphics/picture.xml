    <paragraph xml:id="S2.SS0.SSS0.P1">
      <title>Lines:</title>
      <para xml:id="S2.SS0.SSS0.P1.p1">
        <p>using integers, scaling with floats, LaTeXcounters, TeX counters.
All should be the same.</p>
      </para>
      <para xml:id="S2.SS0.SSS0.P1.p2">
        <picture fill="black" height="100.0pt" stroke="black" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.P1.p2.pic1" tex="\begin{picture}(100.0,100.0)\put(0.0,0.0){\line(1,0){100.0}}\put(0.0,0.0){\line(1,1){50.0}}\put(0.0,0.0){\line(0,1){100.0}}\end{picture}">
          <g transform="translate(0,0)">
            <line points="0,0 100,0" stroke-width="0.4"/>
          </g>
          <g transform="translate(0,0)">
            <line points="0,0 50,50" stroke-width="0.4"/>
          </g>
          <g transform="translate(0,0)">
            <line points="0,0 0,100" stroke-width="0.4"/>
          </g>
        </picture>
        <picture fill="black" height="100.0pt" stroke="black" unitlength="100.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.P1.p2.pic2" tex="\begin{picture}(1.0,1.0)\put(0.0,0.0){\line(1,0){1.0}}\put(0.0,0.0){\line(1,1){0.5}}\put(0.0,0.0){\line(0,1){1.0}}\end{picture}">
